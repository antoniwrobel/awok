{
  "name": "react-mui-ts-starter",
  "version": "0.1.0",
  "description": "React, MUI and TypeScript starter project",
  "private": false,
  "homepage": "./",
  "keywords": [
    "2022",
    "airbnb",
    "editorconfig",
    "emotion",
    "es6",
    "eslint",
    "hooks",
    "materialui",
    "mui",
    "react",
    "roboto",
    "starter",
    "stylelint",
    "typescript",
    "vscode"
  ],
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fontsource/roboto": "4.5.5",
    "@mui/icons-material": "5.6.2",
    "@mui/material": "5.6.4",
    "i18next": "21.8.3",
    "i18next-browser-languagedetector": "6.1.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "11.16.9",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "env-cmd": "10.1.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-jest": "26.2.2",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "postcss-normalize": "10.0.1",
    "stylelint": "14.8.2",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.4",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "build": "env-cmd -f ./env/.env.production react-scripts build",
    "build:dev": "env-cmd -f ./env/.env.development react-scripts build",
    "build:loc": "env-cmd -f ./env/.env.local react-scripts build",
    "build:qa": "env-cmd -f ./env/.env.qa react-scripts build",
    "build:stg": "env-cmd -f ./env/.env.staging react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "start": "env-cmd -f ./env/.env.local react-scripts start",
    "stylelint": "./node_modules/.bin/stylelint \"src/**/*.css\"",
    "test": "env-cmd -f ./env/.env.test react-scripts test --env=jsdom --coverage --watchAll=false"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/index.tsx",
      "!src/util/report-web-vitals.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
